{
    "name": "kitlesoft/aa",
    "description": "Anadolu AjansÄ± Api",
    "keywords": [
        "Kitlesoft",
        "aa"
    ],
    "homepage": "https://github.com/kitlesoft/aa",
    "license": "MIT",
    "authors": [
        {
            "name": "Kitlesoft",
            "email": "info@kitlesoft.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.3",
        "guzzlehttp/guzzle": "^7.5",
        "nesbot/carbon": "^2.62"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Kitlesoft\\Aa\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kitlesoft\\Aa\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
